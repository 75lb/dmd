{
  "name": "@coranos/dmd",
  "author": "coranos",
  "version": "6.0.1",
  "description": "The default output template for jsdoc-to-markdown",
  "license": "MIT",
  "repository": "https://github.com/coranos/dmd",
  "keywords": [
    "documentation",
    "markdown"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "index.js",
    "helpers/*.js",
    "lib/*.js",
    "partials"
  ],
  "scripts": {
    "npm-check-updates": "npm-check-updates --target minor -u",
    "test": "test-runner test/*.js test/ddata/*.js"
  },
  "dependencies": {
    "array-back": "^6.2.2",
    "cache-point": "^2.0.0",
    "common-sequence": "^2.0.2",
    "file-set": "^4.0.1",
    "handlebars": "^4.7.7",
    "marked": "^4.0.10",
    "object-get": "^2.1.1",
    "reduce-flatten": "^3.0.1",
    "reduce-unique": "^2.0.1",
    "reduce-without": "^1.0.1",
    "test-value": "^3.0.0",
    "walk-back": "^5.1.0"
  },
  "devDependencies": {
    "npm-check-updates": "^12.2.1",
    "dmd-plugin-example": "^0.1.0",
    "test-runner": "^0.10.1"
  },
  "standard": {
    "ignore": [
      "example",
      "test/fixture",
      "test/overrides"
    ]
  }
}
